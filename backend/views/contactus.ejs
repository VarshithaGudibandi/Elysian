        <style>
            .topnav {
                overflow: hidden;
                background-color: #282828;
                display:block;
            }

            .topnav a {
              float: left;
              display: inline;
              color: #f2f2f2;
              text-decoration: none;
              font-size: 17px;
            }

            .topnav a:hover {
              background-color: #282828;
              color: #ecbbaa;
            }

            .topnav .icon {
              display: none;
            }

            @media screen and (max-width: 988px) {
              .topnav a:not(:out-of-range){display: none;}
              .topnav a.icon {
                float: right;
                display: block;
              }
            }

            @media screen and (max-width: 988px) {
              .topnav.responsive {position: relative;}
              .topnav.responsive .icon {
                position: relative;
              }
              .topnav.responsive a {
                float: none;
                display: block;
                text-align: right;
                padding: 52px 15px 0px;
              }
            }
        </style>    
    </head>
    
